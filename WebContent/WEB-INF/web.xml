<?xml version="1.0" encoding="UTF-8"?>

<web-app 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee" 
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" 
	version="4.0">
	
	<display-name>App_src</display-name>
	
	<welcome-file-list>
    	<welcome-file>index.jsp</welcome-file>
    	<welcome-file>index.html</welcome-file>
    	<welcome-file>default.html</welcome-file>
    	<welcome-file>default.jsp</welcome-file>   
  	</welcome-file-list>
	
	<servlet-mapping>
    	<servlet-name>default</servlet-name>  
    	<url-pattern>*.css</url-pattern>  
  	</servlet-mapping>
  	
  	
	<servlet>  
    	<servlet-name>Cv_app</servlet-name>  
    	<servlet-class>pack.Cv_app</servlet-class>  
    </servlet>  
	
	<servlet-mapping>  
    	<servlet-name>Cv_app</servlet-name>  
    	<url-pattern>/accueil</url-pattern>  
  	</servlet-mapping>
  	
  	<servlet>  
    	<servlet-name>Depot</servlet-name>  
    	<servlet-class>pack.Depot</servlet-class>  
    </servlet>  
	
	<servlet-mapping>  
    	<servlet-name>Depot</servlet-name>  
    	<url-pattern>/Depot</url-pattern>  
  	</servlet-mapping>
  	
  	<servlet>  
    	<servlet-name>Offres</servlet-name>  
    	<servlet-class>pack.Offres</servlet-class>  
    </servlet>  
	
	<servlet-mapping>  
    	<servlet-name>Offres</servlet-name>  
    	<url-pattern>/Offres</url-pattern>  
  	</servlet-mapping>
  	
  	
  	<servlet>  
    	<servlet-name>PDF2txt</servlet-name>  
    	<servlet-class>pack.PDF2txt</servlet-class>  
    </servlet> 
    
    <servlet-mapping>  
    	<servlet-name>PDF2txt</servlet-name>  
    	<url-pattern>/pdf2txt</url-pattern>  
  	</servlet-mapping>
  	
  	<servlet>  
    	<servlet-name>NEResume</servlet-name>  
    	<servlet-class>pack.NEResume</servlet-class>  
    </servlet> 
    
    <servlet-mapping>  
    	<servlet-name>NEResume</servlet-name>  
    	<url-pattern>/nerresume</url-pattern>  
  	</servlet-mapping>
    
  	
</web-app>